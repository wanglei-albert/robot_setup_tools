<launch>
   <!-- log_level valid values: debug, info, warn, err, fatal -->
   <arg name="log_level" default="info"/>
   <node pkg="robot_setup_tools" name="robot_setup_tools_node" type="robot_setup_tools_node.py">
      <param name="log_level" type="string" value="$(arg log_level)"/>
   </node>
</launch>